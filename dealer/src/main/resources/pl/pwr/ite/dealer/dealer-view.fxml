<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="pl.pwr.ite.dealer.view.controller.DealerController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="200.0">
    <Button fx:id="warehouseConnectButton" mnemonicParsing="false" onAction="#warehouseConnectButtonClick" text="Connect to warehouse">
      <font>
        <Font size="16.0" />
      </font>
      <HBox.margin>
        <Insets left="20.0" />
      </HBox.margin>
    </Button>
  </HBox>
</VBox>
